{
  "name": "cheese-ai-code-frontend",
  "version": "1.0.0",
  "description": "奶酪AI代码平台前端项目",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "build:test": "vite build --mode test",
    "build:prod": "vite build --mode production",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "clean": "rimraf dist node_modules/.vite",
    "openapi2ts": "openapi2ts",
    "api:generate": "npm run openapi2ts",
    "api:clean": "rimraf src/api",
    "api:refresh": "npm run api:clean && npm run api:generate",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "keywords": [
    "vue3",
    "typescript",
    "vite",
    "ant-design-vue",
    "pinia",
    "axios",
    "openapi"
  ],
  "author": "Cheese AI Team",
  "license": "MIT",
  "dependencies": {
    "json-bigint": "^1.0.0",
    "@types/markdown-it": "^14.1.2",
    "ant-design-vue": "^4.2.6",
    "axios": "^1.11.0",
    "dayjs": "^1.11.13",
    "highlight.js": "^11.11.1",
    "markdown-it": "^14.1.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.15.32",
    "@umijs/openapi": "^1.13.15",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.29.0",
    "eslint-plugin-vue": "~10.2.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.5.3",
    "rimraf": "^6.0.1",
    "tslib": "^2.8.1",
    "typescript": "~5.8.0",
    "vite": "^7.0.0",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vue-tsc": "^2.2.10"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
